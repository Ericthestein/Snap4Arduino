{
    "name": "Snap4Arduino-TuneScope connector",
    "description": "Connect Berkeley Snap! to Arduino boards on tunescope.org",
    "version": "1",
    "manifest_version": 2,
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": [
        "serial"
    ],
    "externally_connectable": {
        "matches": [
            "http://dev.arduino.org/*",
            "http://arduino.org/*",
            "http://46.140.176.36/*",
            "http://localhost:8080/*",
            "http://localhost/*",
            "http://snap4arduino.local/*",
            "http://snap4arduino.org/*",
            "http://snap4arduino.rocks/*",
            "https://snap4arduino.rocks/*",

            "https://tunescope.org/*",
            "http://tunescope.org/*",
            "https://tunescope.org/",
            "http://tunescope.org/",
            "https://soundscope-website.web.app/*",
            "http://soundscope-website.web.app/*",
            "https://soundscope-website.web.app/",
            "http://soundscope-website.web.app/",
            "https://soundscope-website.firebaseapp.com/*",
            "http://soundscope-website.firebaseapp.com/*",
            "https://soundscope-website.firebaseapp.com/",
            "http://soundscope-website.firebaseapp.com/"
        ],
        "accepts_tls_channel_id": false
    },
    "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
